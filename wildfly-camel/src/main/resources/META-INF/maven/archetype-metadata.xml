<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="fuse-standard-archetype"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <requiredProperties >
    <requiredProperty key="cxfSupport" >
      <defaultValue>false</defaultValue>
    </requiredProperty>
    <requiredProperty key="pfSupport" >
      <defaultValue>false</defaultValue>
    </requiredProperty>
    <requiredProperty key="jmsSupport" >
      <defaultValue>false</defaultValue>
    </requiredProperty>
    <requiredProperty key="wildfly-camel-version">
      <defaultValue>10.3.0</defaultValue>
    </requiredProperty>
    <requiredProperty key="camel-version">
      <defaultValue>2.24.2</defaultValue>
    </requiredProperty>
    <requiredProperty key="swagger-ui-version">
      <defaultValue>3.24.0</defaultValue>
    </requiredProperty>
    <requiredProperty key="swagger-codegen-version">
      <defaultValue>2.4.10</defaultValue>
    </requiredProperty>
    <requiredProperty key="cxf-codegen-plugin-version">
      <defaultValue>3.3.4</defaultValue>
    </requiredProperty>
    <requiredProperty key="logstash-encoder-version">
      <defaultValue>6.2</defaultValue>
    </requiredProperty>
    <requiredProperty key="primefaces-version">
      <defaultValue>7.0</defaultValue>
    </requiredProperty>
    <requiredProperty key="maven-compiler-plugin-version">
      <defaultValue>3.8.1</defaultValue>
    </requiredProperty>
    <requiredProperty key="maven-war-plugin-version">
      <defaultValue>3.2.3</defaultValue>
    </requiredProperty>
    <requiredProperty key="wildfly-maven-plugin-version">
      <defaultValue>2.0.1.Final</defaultValue>
    </requiredProperty>
    <requiredProperty key="maven-surefire-plugin-version">
      <defaultValue>2.22.2</defaultValue>
    </requiredProperty>
    <requiredProperty key="project-reporting-outputEncoding">
      <defaultValue>UTF-8</defaultValue>
    </requiredProperty>
    <requiredProperty key="project-build-sourceEncoding">
      <defaultValue>UTF-8</defaultValue>
    </requiredProperty>
    <requiredProperty key="maven-compiler-target">
      <defaultValue>1.8</defaultValue>
    </requiredProperty>
    <requiredProperty key="maven-compiler-source">
      <defaultValue>1.8</defaultValue>
    </requiredProperty>
  </requiredProperties>
 <fileSets>
    <fileSet filtered="true" packaged="true" encoding="UTF-8">
      <directory>src/main/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/resources</directory>
      <includes>
        <include>**/*</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/webapp</directory>
      <includes>
        <include>**/*</include>
      </includes>
    </fileSet>
     <fileSet filtered="true" encoding="UTF-8">
         <directory>src/main/fabric8</directory>
         <includes>
             <include>**/*</include>
         </includes>
     </fileSet>
     <fileSet filtered="true" encoding="UTF-8">
         <directory>src/main/fabric8-includes</directory>
         <includes>
             <include>**/*</include>
         </includes>
     </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/data</directory>
      <includes>
        <include>**/*.xml</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" packaged="true" encoding="UTF-8">
      <directory>src/test/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/test/resources</directory>
      <includes>
        <include>**/*</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>ReadMe.md</include>
      </includes>
    </fileSet>
     <fileSet filtered="false" encoding="UTF-8">
         <directory></directory>
         <includes>
             <include>.gitignore</include>
         </includes>
     </fileSet>
  </fileSets>
</archetype-descriptor>
